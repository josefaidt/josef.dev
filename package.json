{
  "name": "josef.dev",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "private": true,
  "workspaces": [
    "support"
  ],
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "node scripts/build",
