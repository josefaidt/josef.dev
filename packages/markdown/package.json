    "rehype-stringify": "^8.0.0",
    "remark": "^13.0.0",
    "remark-frontmatter": "^3.0.0",
    "remark-autolink-headings": "^6.0.1",
    "remark-gfm": "^1.0.0",
    "remark-parse": "^9.0.0",
    "remark-rehype": "^8.1.0",
    "remark-slug": "^6.0.0",
    "to-vfile": "^6.1.0",
    "unified": "^9.2.1"
